    "@vitejs/plugin-react": "^5.1.1",
    "dotenv-cli": "^11.0.0",
    "jsdom": "^27.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
