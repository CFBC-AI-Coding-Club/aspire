{
  "name": "aspire",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3001",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "post-cta-init": "npx create-db@latest",
    "db:generate": "dotenv -e .env.local -- prisma generate",
